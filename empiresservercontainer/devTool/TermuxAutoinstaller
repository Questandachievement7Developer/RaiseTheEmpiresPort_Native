#!/bin/sh
termux-setup-storage
pkg install clang wget -y
wget "https://github.com/Questandachievement7Developer/RaiseTheEmpiresPort_Native/releases/download/Build_193/empires-193" -O empires-server
mv $HOME/empires-server $HOME/storage/downloads
cp ~/storage/downloads/empires-server ~
chmod +x ./empires-server
sh empires-server
